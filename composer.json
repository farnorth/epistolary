{
    "name": "pilaster/epistolary",
    "description": "Laravel Newsletter Management",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Ryan Winchester",
            "email": "fungku@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "ramsey/uuid": "^3.5",
        "guzzlehttp/guzzle": "~5.3|~6.0"
    },
    "require-dev": {
        "laravel/framework": "5.2.*|5.3.*",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "tightenco/mailthief": "^0.3.1"
    },
    "autoload": {
        "psr-4": {
            "Pilaster\\Epistolary\\": "src/"
        },
        "files": ["src/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "RC",
    "prefer-stable": true
}
