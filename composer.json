{
    "name": "pilaster/newsletters",
    "description": "Laravel Newsletter Management",
    "authors": [
        {
            "name": "Ryan Winchester",
            "email": "fungku@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "laravel/framework": "5.2.*|5.3.*",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "tightenco/mailthief": "^0.3.1"
    },
    "autoload": {
        "psr-4": {
            "Pilaster\\Newsletters\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "RC",
    "prefer-stable": true
}
